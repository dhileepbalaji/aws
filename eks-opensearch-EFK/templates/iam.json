{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "es:ESHttp*"
            ],
            "Resource": "arn:aws:es:__AWS_REGION__:__ACCOUNT_ID__:domain/__ES_DOMAIN_NAME__",
            "Effect": "Allow"
        },
        {
            "Action": "sts:AssumeRole",
            "Effect": "Allow",
            "Resource": "*"
        }
    ]
}